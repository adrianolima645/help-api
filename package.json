{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": "v17.4.0"
  },
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules ./src/server.ts",
    "dev:watch": "rimraf ./dist && tsc -w && watch-node",
    "build": "rimraf ./dist && yarn tsc",
    "start": "node ./dist/server.js",
    "watch-node": "nodemon ./dist/server.js",
    "watch-ts": "rimraf ./dist && tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku main",
    "tsc": "./node_modules/typescript/bin/tsc"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "mongoose": "^6.7.0",
    "multer": "^1.4.4",
    "swagger-ui-express": "^4.3.0",
    "typescript": "4.7.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.8",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.20",
    "ts-node-dev": "^1.1.8",
    "tsc": "^2.0.4"
  }
}
